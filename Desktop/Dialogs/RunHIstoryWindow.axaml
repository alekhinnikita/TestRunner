<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="Desktop.Dialogs.RunHIstoryWindow"
        Title="RunHIstoryWindow">
	<Grid>
		<DataGrid Name="DataGrid">
			<DataGrid.Columns>
				<DataGridTextColumn Header="Название" Binding="{Binding Path=Name}"/>
				<DataGridTextColumn Header="Файл" Binding="{Binding Path=File}"/>
				<DataGridTextColumn Header="Результат" Binding="{Binding Path=Success}"/>
				<DataGridTextColumn Header="Время" Binding="{Binding Path=DateTime}"/>
			</DataGrid.Columns>
		</DataGrid>
	</Grid>
</Window>
